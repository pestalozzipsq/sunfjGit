<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunfj.dao.AdminRecoMapper">
    <resultMap id="BaseResultMap" type="com.sunfj.entity.AdminReco">
        <id column="AUTH_RECO_ID" jdbcType="VARCHAR" property="recoId" />
        <result column="SYS_ID" jdbcType="VARCHAR" property="sysId" />
    </resultMap>

    <select id="getAdminReco" resultType="AdminReco" parameterType="String">
        select AUTH_RECO_ID,SYS_ID from admin_sm_auth_reco where AUTH_RECO_ID = #{id}
    </select>
</mapper>